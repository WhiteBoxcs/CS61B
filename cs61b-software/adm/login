# Environment variables for CS61B home use (Linux and MacOS with bash shell).
# Source this from your $HOME/.bash_profile and $HOME/.profile files.

# This assumes the cs61b-software directory is in your home directory.
# Change the following line if you choose a different directory from
# $HOME/cs61b-software to put cs61b software.
MASTERDIR=$HOME/cs61b-software
export MASTERDIR

PATH=${PATH}:${MASTERDIR}/bin

CLASSPATH=${CLASSPATH:-.}
CLASSPATH="${CLASSPATH}:$MASTERDIR/lib/*"
export CLASSPATH
